{
  "name": "restaurant-inspections-webapp",
  "version": "0.0.0",
  "description": "\"Restaurant inspections Web App\"",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install && ./node_modules/.bin/bourbon install",
    "lite": "lite-server",
    "typings": "typings",
    "build:css": "node-sass src/css/ --output dist/ --include-paths node_modules/bourbon/app/assets/stylesheets",
    "build": "browserify src/ts/main.ts -p tsify -t babelify -o dist/bundle.js",
    "watch": "watchify src/ts/main.ts -p tsify -t babelify -o dist/bundle.js",
    "start": "npm run build && concurrently \"npm run watch\" \"npm run lite\""
  },
  "author": "\"Marcos Hern√°ndez <marcoslhc@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.2",
    "@angular/upgrade": "^2.0.0-rc.5",
    "babelify": "^7.3.0",
    "bourbon": "^4.2.7",
    "browserify": "^13.1.0",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.8.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "tsify": "^1.0.3",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "zone.js": "^0.6.12"
  }
}
